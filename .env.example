# SynthQuant API Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# DATABASE CONFIGURATION (Supabase PostgreSQL)
# ===========================================
# Get this from Supabase Dashboard > Settings > Database > Connection string
# Use the "URI" format and replace [YOUR-PASSWORD] with your database password
# Format: postgresql+asyncpg://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/synthquant

# ===========================================
# AUTHENTICATION
# ===========================================
# Admin secret for creating new API keys (change in production!)
ADMIN_SECRET=dev_admin_secret_change_in_production

# Initial API key to be added to valid keys on startup
INITIAL_API_KEY=sk-synthquant-prod-001

# ===========================================
# RATE LIMITING
# ===========================================
# Maximum requests per time window
RATE_LIMIT_REQUESTS=10

# Time window in seconds
RATE_LIMIT_WINDOW_SECONDS=60

# ===========================================
# API CONFIGURATION
# ===========================================
# Base URL for generating download links
API_BASE_URL=http://localhost:8000

# ===========================================
# EXTERNAL SERVICES (Optional)
# ===========================================
# Alpha Vantage API key for additional market data (optional)
ALPHA_VANTAGE_API_KEY=
